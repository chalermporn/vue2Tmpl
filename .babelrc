{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "ie >= 9"]
      },
      "modules": false
    }],
    "stage-2"
  ],
  "plugins": ["transform-runtime", ["component", [{
    "libraryName": "element-ui",
    "styleLibraryName": "theme-chalk"
  }]]],
  "comments": false,
  "env": {
    "test": {
      "plugins": [ "istanbul" ]
    }
  }
}
